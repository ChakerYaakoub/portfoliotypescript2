{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "projects": {
    "default": "portfoliotypescript",
    "admin-panel": {
      "projectId": "admin-panel-project-id"
    },
    "user-interface": {
      "projectId": "user-interface-project-id"
    }
  }
}
